    <!--  类似购物车数量加减 按钮  -->
    <declare-styleable name="shopNumberView">
        <attr name="maxValue" format="dimension" />
        <attr name="canInput" format="boolean" />
    </declare-styleable>

    <!--    华为loading自定义属性： 控件大小，控件颜色，转一圈的时间-->
    <declare-styleable name="HuaWeiLoadingView">
        <attr name="hw_size" format="dimension" />
        <attr name="hw_color" format="color" />
        <attr name="hw_duration" format="integer" />
    </declare-styleable>
